---
import '@/styles/globals.css';

import Layout from '../layouts/Layout.astro';
import Test from '../components/Test';
import Sheets from '../components/Sheets.astro';
// https://docs.google.com/spreadsheets/d/1Y1e2IONJqSt1jPE4-DNrzGjR4OSXunAMGBKf_Q9DiVU/edit?gid=0#gid=0
const response = await fetch(
  'https://opensheet.elk.sh/1Y1e2IONJqSt1jPE4-DNrzGjR4OSXunAMGBKf_Q9DiVU/1',
);
const data = await response.json();
---

<Layout title="Welcome to Astro.">
  <Test data={data} client:load />
</Layout>
